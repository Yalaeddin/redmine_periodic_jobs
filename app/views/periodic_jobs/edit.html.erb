<h2><%= link_to l(:label_periodic_job_plural), periodic_jobs_path %> &#187; <%=h @periodic_job.title %></h2>

<%= labelled_form_for @periodic_job, :url => {:controller=>'periodic_jobs', :action=>'update'}, :html => { :class => nil, :method => :put } do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
	<p><%= submit_tag l(:button_update) %></p>
<% end %>
